# =============================================================================
# VelesDB - Environment Variables
# =============================================================================
# Copy this file to .env and fill in your values
# NEVER commit .env to git!
# =============================================================================

# Crates.io API Token
# Get from: https://crates.io/settings/tokens
# Scopes needed: publish-new, publish-update
CARGO_REGISTRY_TOKEN=

# GitHub Token for Premium repo notifications (optional)
# PREMIUM_REPO_TOKEN=

# =============================================================================
# Migration Testing (velesdb-migrate)
# =============================================================================
# For testing migrations with real data sources

# Supabase
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_KEY=your-service-role-key
SUPABASE_TABLE=your_table_name
SUPABASE_VECTOR_COL=embedding
SUPABASE_ID_COL=id

# Qdrant (optional)
# QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=
# QDRANT_COLLECTION=
